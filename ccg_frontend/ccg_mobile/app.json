{
  "expo": {
    "name": "ccg_mobile",
    "slug": "concordiacampusguide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow the app to access your location at all times.",
          "locationWhenInUsePermission": "Allow the app to access your location while using the app."
        }
      ],
      "expo-localization"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ikozay.concordiacampusguide",
      "infoPlist": {
      "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCVUC2dAFA9M1nZ4--XHM3GUMWKcEgbgVU"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "experimental": {
        "useFabric": false
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCVUC2dAFA9M1nZ4--XHM3GUMWKcEgbgVU"
        }
      },
      "package": "com.ikozay.concordiacampusguide"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b9b79a0c-b3d2-47e5-b0d9-f7c4250c5d82"
      }
    },
    "owner": "ikozay"
  }
}
